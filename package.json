{
  "name": "gta-browser-game",
  "version": "1.0.0",
  "description": "Ultimate GTA Browser Game with Three.js - Multiplayer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "tsc && vite build",
    "build:server": "cd server && npm run build",
    "build:all": "npm run build:server && npm run build",
    "preview": "vite preview",
    "start:server": "cd server && npm run start"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.19.3",
    "cannon-es": "^0.20.0",
    "colyseus.js": "^0.15.25",
    "gsap": "^3.12.4",
    "howler": "^2.2.4",
    "postprocessing": "^6.38.2",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/howler": "^2.2.12",
    "@types/three": "^0.160.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}
